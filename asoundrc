pcm.!default {
    type plug
        slave.pcm "asymed"
}

# improve sound quality
defaults.pcm.rate_converter "speexrate_best"

pcm.asymed {
    type asym
        playback.pcm {
            @func getenv
                vars [ ALSAPCM ]
                default "dmix"
        }
    capture.pcm "dsnoop"
}
