{
    "editor.fontFamily": "Iosevka Term Medium",
    "editor.fontSize": 17.5,
    "window.zoomLevel": 0.9,
    "workbench.colorTheme": "Gruvbox Dark (Medium)",
    "vim.statusBarColors.visual": "#5f0000",
    "workbench.statusBar.visible": true,
    "editor.formatOnSave": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#282828",
        "statusBar.border": "#928374",
        "panel.background": "#1d2021",
        "sideBar.background": "#282828",
        "editor.background": "#282828",
        "scrollbarSlider.activeBackground": "#444444",
        "scrollbarSlider.background": "#3c3836",
        "scrollbarSlider.hoverBackground": "#372828ef",
        "scrollbar.shadow": "#444444",
        "editor.lineHighlightBackground": "#35322b",
        "editorGutter.background": "#313131"
    },
    "workbench.statusBar.feedback.visible": false,
    "editor.lineNumbers": "relative",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "build": true,
        ".vscode": true
    },
    "C_Cpp.loggingLevel": "Debug",
    "window.menuBarVisibility": "toggle",
    "workbench.editor.showTabs": true,
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.formatting": "Disabled",
    "uncrustify.configPath": "$HOME/.config/uncrustify/uncrustify.cfg",
    "uncrustify.executablePath": "/usr/bin/uncrustify",
    "uncrustify.graphicalConfig": false
}